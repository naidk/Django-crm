[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/Django-CRM-mastery-app-Project-1-YT-main
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/Django-CRM-mastery-app-Project-1-YT-main/dev/crm/crm.sock crm.wsgi:application

       
[Install]
WantedBy=multi-user.target